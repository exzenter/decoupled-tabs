{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "decoupled-tabs/tab-trigger",
    "version": "1.0.0",
    "title": "Tab Trigger",
    "category": "layout",
    "icon": "button",
    "description": "A wrapper block that triggers a tab when clicked. Add any content inside.",
    "keywords": [
        "tab",
        "trigger",
        "button",
        "link"
    ],
    "textdomain": "decoupled-tabs",
    "attributes": {
        "tabTarget": {
            "type": "string",
            "default": ""
        },
        "tabArea": {
            "type": "string",
            "default": ""
        },
        "cssDefault": {
            "type": "string",
            "default": ""
        },
        "cssHover": {
            "type": "string",
            "default": ""
        },
        "cssActive": {
            "type": "string",
            "default": ""
        },
        "triggerId": {
            "type": "string",
            "default": ""
        }
    },
    "supports": {
        "html": false,
        "align": false,
        "spacing": {
            "margin": true,
            "padding": true
        }
    },
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css"
}